{
  "name": "postcss-require-hover",
  "version": "0.1.0",
  "description": "PostCSS plugin that wraps hover selectors in a media media that's only valid on devices that support true hover events.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "hover"
  ],
  "author": "Jack Burns <github@jackburns.dev>",
  "license": "MIT",
  "repository": "jackburns/postcss-require-hover",
  "bugs": {
    "url": "https://github.com/jackburns/postcss-require-hover/issues"
  },
  "homepage": "https://github.com/jackburns/postcss-require-hover",
  "dependencies": {
    "postcss": "^6.0.1"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-postcss": "^2.0.2",
    "jest": "^20.0.0"
  },
  "scripts": {
    "test": "jest && eslint *.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es5",
    "env": {
      "jest": true
    }
  }
